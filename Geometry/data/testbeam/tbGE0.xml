<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../../DetectorDescription/Schema/DDLSchema.xsd">
    <ConstantsSection label="ge0.xml" eval="true">
        <Constant name="layers" value="5" />
        <Constant name="dxSC" value="0.0*cm" />
        <Constant name="dySC" value="30.0*cm" />
        <Constant name="dzSC" value="23.0*cm" />
        <Constant name="separationSC" value="3.47*cm" />
        <Constant name="dyBox" value="150*cm" />
        <Constant name="dxBox" value="200*cm" />
        <Constant name="dzBox" value="200*cm" />
        <Constant name="dx" value="0.0*cm" />
        <Constant name="rz" value="0.0*deg" />
    </ConstantsSection>

    <RotationSection label="ge0.xml">
        <RotationSequence name="y90">
            <RotationByAxis axis="y" angle="-90*deg" />
        </RotationSequence>
    </RotationSection>

    <PosPartSection label="ge0.xml">
    <!-- main Translation done in mf.xml -->
        <PosPart copyNumber="1">
            <rParent name="ge0:GE0P" />
            <rChild name="ge0:GE0Box" />
            <rRotation name="y90" />
        </PosPart>
    </PosPartSection>
</DDDefinition>
